{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node",
        "config": { "includeFiles": ["dist/**"] }
      }
    ],
    "routes": [
        {
          "src": "/about",
          "dest": "/views/about.html"
        },
        {
          "src": "/",
          "dest": "/views/home.html"
        },
        {
          "src": "/(.*)",
          "dest": "server.js"
        }
      ]
    }